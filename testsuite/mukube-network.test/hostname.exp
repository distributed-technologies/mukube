
# The hostname should be specified at buildtime. 
# I need to read the correct hostname from the config files.

set test_name "hostname"

spawn hostname 

expect {
    ITMDKLT097 {
        pass $test_name 
    }
    eof {
        fail "$test_name $expect_out(buffer)" 
    }
    timeout {
        fail "$test_name timed out." 
    }
}
