set test_name "Ping example.com"

send "\r"
expect #

send "ping example.com -c 1\r"

expect {
    "packets transmitted" {
        pass $test_name
    }
    failure {
        fail "$test_name $expect_out(0,string)"
    }
    eof {
        unresolved "$test_name $expect_out(0,string)"
    }
    timeout {
        fail "$test_name timed out"
    }
}
